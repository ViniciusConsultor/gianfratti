	</tab>
	<tab name="Toolbar">
		<param name="toolbar_color" type="color" description="Text color" default="#FFFFFF"/>
		<param name="toolbar_background" type="color" description="Background color" default="#999999"/>
		<param name="toolbar_background1" type="color" description="Gradient start color" default="#F0F0F0"/>
		<param name="toolbar_background2" type="color" description="Gradient end color" default="#333333"/>
