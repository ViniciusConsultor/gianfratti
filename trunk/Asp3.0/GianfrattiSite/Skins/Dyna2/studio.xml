<skin>
	<tab name="General">
		<param name="general_font" type="font" description="Font" default="verdana"/>
		<param name="body_width" type="number" description="Size (if pct add %)" default="100%"/>
		<param name="leftmargin_size" type="number" description="Left margin size" default="170"/>
		<param name="rightmargin_size" type="number" description="Right margin size" default="180"/>
		<param name="body_align" type="valign" description="Alignment" default="center"/>
	</tab>
	<tab name="Body">
		<param name="body_background" type="color" description="Background color" default="#FFFFFF"/>
		<param name="body_color" type="color" description="Normal text color" default="#000000"/>
		<param name="bodytext_color" type="color" description="Body text color" default="#000000"/>
