		<param name="page_background" type="color" description="Page background color" default="#EEEEEE"/>
		<param name="pagetitle_color" type="color" description="Pagetitle color" default="#000099"/>
	</tab>
	<tab name="Title">
		<param name="title_background" type="color" description=" Title background color" default="#000099"/>
